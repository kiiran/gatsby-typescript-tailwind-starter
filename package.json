{
  "name": "gatsby-typescript-tailwind-starter",
  "private": true,
  "description": "A starter for a blog powered by Gatsby and Markdown, with TS and Tailwind",
  "version": "0.1.0",
  "author": "Ciaran Edwards <hello@ciaran.codes>",
  "homepage": "https://github.com/kiiran/gatsby-typescript-tailwind-starter#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiiran/gatsby-typescript-tailwind-starter.git"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "run-p develop codegen:watch",
    "codegen": "graphql-codegen --config graphql-codegen.yml",
    "codegen:watch": "npm run codegen -- --watch src/**/*.{ts,tsx} --silent",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint": "eslint ./*.js src --ext js,ts,tsx --fix --quiet",
    "start": "npm run dev",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "typecheck": "tsc -p ./tsconfig.json"
  },
  "dependencies": {
    "@fontsource/lora": "^4.3.0",
    "@fontsource/manrope": "^4.3.0",
    "gatsby": "^3.5.0",
    "gatsby-plugin-feed": "^3.5.0",
    "gatsby-plugin-gatsby-cloud": "^2.5.0",
    "gatsby-plugin-image": "^1.5.0",
    "gatsby-plugin-manifest": "^3.5.0",
    "gatsby-plugin-offline": "^4.5.0",
    "gatsby-plugin-react-helmet": "^4.5.0",
    "gatsby-plugin-sharp": "^3.5.0",
    "gatsby-remark-copy-linked-files": "^3.2.0",
    "gatsby-remark-images": "^4.2.0",
    "gatsby-remark-prismjs": "^4.2.0",
    "gatsby-remark-responsive-iframe": "^3.2.0",
    "gatsby-remark-smartypants": "^3.2.0",
    "gatsby-source-filesystem": "^3.5.0",
    "gatsby-transformer-remark": "^3.2.0",
    "gatsby-transformer-sharp": "^3.5.0",
    "prismjs": "^1.23.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/fragment-matcher": "^2.0.1",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-document-nodes": "^1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@tailwindcss/typography": "^0.4.0",
    "@types/react-helmet": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^3.10.2",
    "gatsby-plugin-postcss": "^4.5.0",
    "gatsby-plugin-resolve-src": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.15",
    "prettier": "2.2.1",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.4"
  }
}
